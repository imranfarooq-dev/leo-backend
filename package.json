{
  "name": "leo-backend",
  "version": "0.1.4",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build && npm run sentry:sourcemaps",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/src/main",
    "start:debug": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org leo-xx --project leo-backend --release $npm_package_name@$npm_package_version ./dist && sentry-cli sourcemaps upload --org leo-xx --project leo-backend --release $npm_package_name@$npm_package_version ./dist"
  },
  "dependencies": {
    "@clerk/express": "^1.3.56",
    "@nestjs/bull": "^11.0.2",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@sentry/nestjs": "^9.7.0",
    "@sentry/profiling-node": "^9.7.0",
    "@sentry/tracing": "^7.120.3",
    "@supabase/supabase-js": "^2.49.1",
    "archiver": "^7.0.1",
    "axios": "^1.7.2",
    "bull": "^4.16.5",
    "canvas": "^3.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "lodash": "^4.17.21",
    "node-addon-api": "^8.3.1",
    "node-gyp": "^11.1.0",
    "openai": "^4.52.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "stripe": "^17.3.1",
    "svix": "^1.29.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "unpdf": "^0.12.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@sentry/cli": "^2.42.4",
    "@types/archiver": "^6.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.16",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.17.24",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
